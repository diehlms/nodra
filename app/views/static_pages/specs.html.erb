    <div class="spec-tables">
      <div class="hull-table">
        <table class="table table-hover">
          <thead>
            <tr>
              <th scope="col">Hull</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">Length Overall</th>
              <td>17' 0"</td>
            </tr>
            <tr>
              <th scope="row">Length at Waterline</th>
              <td>10' 6"</td>
            </tr>
            <tr>
              <th scope="row">Beam</th>
              <td colspan="2">5' 8"</td>
            </tr>
            <tr>
              <th scope="row">Maximum Draft</th>
              <td colspan="2">3' 6"</td>
            </tr>
            <tr>
              <th scope="row">Minimum Weight</th>
              <td colspan="2">400 lbs</td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="sail-table">
        <table class="table table-hover">
          <thead>
            <tr>
              <th scope="col">Sail Area</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">Main</th>
              <td>78 sq. ft.</td>
            </tr>
            <tr>
              <th scope="row">Jib</th>
              <td>59 sq. ft.</td>
            </tr>
            <tr>
              <th scope="row">Total</th>
              <td colspan="2">137 sq. ft.</td>
            </tr>
            <tr>
              <th scope="row">Mast Height (from deck)</th>
              <td colspan="2">24' 7"</td>
            </tr>
            <tr>
